{
  "name": "@worldql/client",
  "version": "0.0.7",
  "description": "TypeScript Client for interacting with WorldQL over WebSockets",
  "type": "module",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "files": [
    "lib/**/*"
  ],
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "keywords": [
    "worldql",
    "typescript",
    "client",
    "websockets",
    "message broker",
    "spatial database"
  ],
  "scripts": {
    "prepublishOnly": "yarn run test && yarn run build",
    "test": "yarn run check && yarn run lint",
    "check": "yarn run compile --noEmit",
    "lint": "xo ./src",
    "clean": "rimraf lib",
    "compile": "tsc",
    "prebuild": "yarn run clean",
    "build": "yarn run compile",
    "predev": "yarn run clean",
    "dev": "yarn run compile --watch"
  },
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/uuid": "^8.3.3",
    "@types/ws": "^8.2.0",
    "rimraf": "^3.0.2",
    "type-fest": "^2.5.4",
    "typescript": "^4.5.2",
    "xo": "^0.46.4"
  },
  "dependencies": {
    "eventemitter3": "^4.0.7",
    "flatbuffers": "^2.0.4",
    "isomorphic-ws": "^4.0.1",
    "ws": "^8.2.3"
  }
}
